{
  "expo": {
    "name": "RotuClaro",
    "slug": "rotuclaro-tcc",
    "userInterfaceStyle": "light",
    "version": "0.3.0",
    "orientation": "portrait",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#4ba363"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#4ba363"
      },
      "package": "com.ifsp.rotuclaro"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
